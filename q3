// Q3: Interchanging second with second last element
#include <stdio.h>
int main() {
    int n, i, temp;
    printf("Enter size:\n");
    scanf("%d", &n);
    int arr[n];
    for(i=0;i<n;i++) scanf("%d",&arr[i]);
    if(n>=2){
        temp=arr[1];
        arr[1]=arr[n-2];
        arr[n-2]=temp;
    }
    printf("Resulting array:\n");
    for(i=0;i<n;i++) printf("%d ", arr[i]);
    return 0;
}
