// Q9: Product of elements above main diagonal
#include <stdio.h>
int main(){
    int n,i,prod=1;
    printf("Order: ");
    scanf("%d",&n);
    int a[n][n];
    for(i=0;i<n*n;i++) scanf("%d",&a[0][i]);
    for(i=0;i<n-1;i++) prod*=a[i][i+1];
    printf("Product above main diagonal: %d",prod);
    return 0;
}
