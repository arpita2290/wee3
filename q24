// Q24: Find frequency of each element in an array
#include <stdio.h>
int main(){
    int n,i,j,count;
    scanf("%d",&n);
    int a[n],freq[n];
    for(i=0;i<n;i++) scanf("%d",&a[i]);
    for(i=0;i<n;i++) freq[i]=0;
    for(i=0;i<n;i++){
        count=1;
        if(freq[i]) continue;
        for(j=i+1;j<n;j++) if(a[i]==a[j]) {count++;freq[j]=1;}
        printf("%d appears %d times\n",a[i],count);
    }
    return 0;
}
