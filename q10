// Q10: Count total number of non-zero elements in a 2D array
#include <stdio.h>
int main(){
    int r,c,i,j,count=0;
    printf("Rows, Columns: ");
    scanf("%d %d",&r,&c);
    int a[r][c];
    for(i=0;i<r;i++) for(j=0;j<c;j++) scanf("%d",&a[i][j]);
    for(i=0;i<r;i++) for(j=0;j<c;j++) if(a[i][j]!=0) count++;
    printf("Non-zero count: %d",count);
    return 0;
}
