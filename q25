// Q25: Find and print the sum of each row and each column in a matrix
#include <stdio.h>
int main(){
    int r,c,i,j;
    scanf("%d%d",&r,&c);
    int a[r][c];
    for(i=0;i<r;i++) for(j=0;j<c;j++) scanf("%d",&a[i][j]);
    for(i=0;i<r;i++){
        int s=0;
        for(j=0;j<c;j++) s+=a[i][j];
        printf("Sum row %d: %d\n",i+1,s);
    }
    for(j=0;j<c;j++){
        int s=0;
        for(i=0;i<r;i++) s+=a[i][j];
        printf("Sum col %d: %d\n",j+1,s);
    }
    return 0;
}
