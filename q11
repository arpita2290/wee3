// Q11: Store all odd elements and even elements of 2D array in separate arrays
#include <stdio.h>
int main() {
    int r,c,i,j,odd[100],even[100],oco=0,eco=0;
    scanf("%d%d",&r,&c);
    int a[r][c];
    for(i=0;i<r;i++) for(j=0;j<c;j++) {
        scanf("%d",&a[i][j]);
        if(a[i][j]%2) odd[oco++]=a[i][j];
        else even[eco++]=a[i][j];
    }
    printf("Odd: "); for(i=0;i<oco;i++) printf("%d ",odd[i]);
    printf("\nEven: "); for(i=0;i<eco;i++) printf("%d ",even[i]);
    return 0;
}
