// Q2: Display all pairs of elements whose sum is 50 (100 int array)
#include <stdio.h>
int main() {
    int arr[100], i, j;
    printf("Enter 100 numbers:\n");
    for (i=0; i<100; i++)
        scanf("%d", &arr[i]);
    printf("Pairs whose sum is 50:\n");
    for (i=0; i<99; i++)
        for (j=i+1; j<100; j++)
            if (arr[i]+arr[j]==50)
                printf("(%d, %d)\n", arr[i], arr[j]);
    return 0;
}
